generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RequestLog {
  id         Int      @id @default(autoincrement())
  path       String
  created_at DateTime @default(now()) @db.Timestamptz(6)

  @@map("request_log")
}
